<UserControl x:Class="HMI.UserControls.MV_R"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vw="http://inosoft.com/visiwin7"
        Width="46" Height="108" Background="Transparent" >

    <Grid >
        <vw:PictureBox x:Name="B" Width="46" Height="108" Symbol="{DynamicResource R1}" />
        <vw:PictureBox x:Name="R1" Width="46" Height="108" SymbolList="{DynamicResource MVRack1}"  />
        <vw:PictureBox x:Name="R2" Width="46" Height="108" SymbolList="{DynamicResource MVRack2}" />

        <vw:Button x:Name="R"  Click="OpenOvenView" Background="Transparent" BorderThickness="0" Text="" AuthorizationRight="Status"/>
    </Grid>
</UserControl>
