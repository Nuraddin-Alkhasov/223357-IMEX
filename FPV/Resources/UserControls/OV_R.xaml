<UserControl x:Class="HMI.UserControls.OV_R"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vw="http://inosoft.com/visiwin7"
        Width="228" Height="200" Background="Transparent" >

    <Grid >
        <vw:PictureBox x:Name="B" Symbol="{DynamicResource R2}" />
        <vw:PictureBox x:Name="R1"  SymbolList="{DynamicResource OVRack2}"/>
        <vw:PictureBox x:Name="R2"  SymbolList="{DynamicResource OVRack1}"/>
    </Grid>
</UserControl>
