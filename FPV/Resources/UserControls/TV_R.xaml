<UserControl x:Class="HMI.UserControls.TV_R"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vw="http://inosoft.com/visiwin7"
        Width="168" Height="310" Background="Transparent" >

    <Grid >
        <vw:PictureBox x:Name="B" Symbol="{DynamicResource R3}" />
        <vw:PictureBox x:Name="R1"  SymbolList="{DynamicResource TVRack2}" />
        <vw:PictureBox x:Name="R2"  SymbolList="{DynamicResource TVRack1}" />
        <vw:Button x:Name="R"  Click="OpenOvenView" Background="Transparent" BorderThickness="0" Text="" AuthorizationRight="Status"/>
    </Grid>
</UserControl>
